{
    "name": "franzip/serp-fetcher",
    "type": "library",
    "description": "Wrapper around SimpleHtmlDom to easily fetch data from Search Engine Result Pages with built-in caching support.",
    "license": "MIT",
    "homepage": "http://github.com/franzip/serp-fetcher",
    "keywords": ["serp", "fetch", "data", "search-engine", "simplehtmldom", "cache"],
    "authors": [{
        "name": "Francesco Pezzella",
        "email": "franzpezzella@gmail.com"
    }],
    "require": {
        "php": ">=5.3.3",
        "thauex/simple-html-dom": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Franzip\\SerpFetcher\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
